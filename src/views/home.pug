- if (session.error)
  h2 Not Authenticated
  #register
    a(href='/register') Register
  #password-login
    a(href='/login', style='border: 0px') Login with Password
  h3
    a(href='/logout') Logout
  pre=JSON.stringify(session, null, 2)
- else
  h2 Authenticated
  h3
    a(href='/logout') Logout
  pre=JSON.stringify(session, null, 2)